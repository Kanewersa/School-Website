$("<%=j render(:partial => 'events', :object => @events) %>")
    .appendTo($("#paginate"));

// Update pagination link
<% if @events.last_page? %>
$('.pagination').html("To już wszystko!");
window.loadingMore = true;
<% else %>
$('.pagination').html("<%=j link_to_next_page(@events, 'Ładowanie...', :remote => true, :id => "load-more-link") %>");
window.loadingMore = false;
<% end %>